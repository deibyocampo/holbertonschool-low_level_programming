#include "holberton.h"
#include <stdio.h>
/**
 * times_table - print result of multiplication of two numbers.
 *
 * Return: numbers of higher to lower.
 */
void print_to_98(int n)
{
while (n < 98)
{
printf("%d, ", n);
n++;
}
while (n > 98)
{
printf("%d, ", n);
n--;
}
if (n == 98)
{
printf("%d\n", n);
}
}
